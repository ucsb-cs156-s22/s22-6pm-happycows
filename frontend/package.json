{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.0-beta3",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^2.0.0-alpha.0",
    "react-dom": "^17.0.1",
    "react-json-view": "^1.21.3",
    "react-query": "^3.12.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.2.7",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.2.7",
    "@stryker-mutator/core": "^4.6.0",
    "@stryker-mutator/jest-runner": "^4.6.0",
    "@testing-library/react-hooks": "^5.1.2",
    "axios-mock-adapter": "^1.19.0",
    "env-cmd": "^10.1.0",
    "jest-mock-console": "^1.1.0",
    "react-test-renderer": "^17.0.2"
  },
  "scripts": {
    "start": "env-cmd -f ../.env -e development react-scripts start",
    "build": "env-cmd -f ../.env --silent react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "storybook": "start-storybook --docs -p 6006 -s public",
    "build-storybook": "build-storybook --docs -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/main/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
